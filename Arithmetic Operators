#include<stdio.h>
#include<math.h>
int main()
{
  int x,n,a;
  float z;
    printf("Enter 2 Nos\n : ");
    scanf("%d%d",&x,&n);
    a=x+n;
    printf("add %d\n",a);
    a=x-n;
    printf("sub %d\n",a);
    a=x*n;
    printf("Mul %d\n",a);
    z=(float)x/(float)n;
    printf("Div %0.2f\n",z);
    a=pow(x,n);
    printf("Pow %d\n",a);
}
